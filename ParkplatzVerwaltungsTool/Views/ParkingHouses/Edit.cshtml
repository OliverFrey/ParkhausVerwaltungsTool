@model ParkplatzVerwaltungsTool.Models.ParkingHouse

@{
    ViewData["Title"] = "Parkhaus bearbeiten";
}

<h1>Parkhaus bearbeiten</h1>

<h4>Parkhaus</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ParkingHouseId" />
            <div class="form-group">
                <label asp-for="ParkingHouseName" class="control-label"></label>
                <input asp-for="ParkingHouseName" class="form-control" />
                <span asp-validation-for="ParkingHouseName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Zurück zur Liste</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
